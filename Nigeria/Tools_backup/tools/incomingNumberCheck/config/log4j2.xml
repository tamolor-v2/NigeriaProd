<?xml version="1.0" encoding="utf-8"?>
<Configuration monitorInterval="300">
  <Properties>
    <Property name="log-path">/mnt/beegfs/yousef/incomingNumberCheck/log</Property>
  </Properties>
  <Appenders>
    <Console name="CONSOLE" target="SYSTEM_OUT">
      <PatternLayout pattern="%5p [%t] - %m%n" />
    </Console>
  </Appenders>
  <Loggers>
    <Root level="INFO">
      <AppenderRef ref="CONSOLE" />
    </Root>
  </Loggers>
</Configuration>
