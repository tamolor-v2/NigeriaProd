
select to_clob(activity_name) || chr(001) || to_clob(event_ref_no) || chr(001) || to_clob("SERVICE_ID/USER_ID") || chr(001) || to_clob(created_by) || chr(001) || to_clob(created_user_source) || chr(001) || to_clob(location_desc) || chr(001) || to_clob(created_date_time) || chr(001) || to_clob(assigned_date_time) || chr(001) || to_clob(closed_date_time) || chr(001) || to_clob(status) || chr(001) || to_clob("STATUS MODIFIED_BY") || chr(001) || to_clob(ticket_type)|| chr(001) || to_clob(catecory) || chr(001) || to_clob(sub_category) || chr(001) || to_clob(translate(issue_note, chr(10)||chr(11)||chr(13) , ' ')) || chr(001) || to_clob(translate(resolution_note, chr(10)||chr(11)||chr(13) , ' ')) || chr(001) || to_clob(to_char(to_date(CREATED_DATE_TIME,'DD-MON-YYYY HH24:MI:SS'),'yyyymmdd')) from TT_MSO_1.CALL_REASON_JAN_18_VW where SUBSTR (event_ref_no,-1,1)='${msisdn}'
